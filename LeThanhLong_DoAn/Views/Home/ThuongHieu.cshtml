@model List<LeThanhLong_DoAn.Models.ThuongHieu>


<div class="container" style="padding: 20px 0px; background-image: linear-gradient(white, rgb(25, 117, 147), aqua)">
    <h4>Thương hiệu nổi bậc</h4>
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-md-4 col-sm-4">
                <a href="/home/sanphamtheothuonghieu/@item.MaThuongHieu"><img src="~/img/@item.HinhAnh" style="width:100%; height:100%; border-radius:5px" /></a>               
            </div>
        }
    </div>
</div>
